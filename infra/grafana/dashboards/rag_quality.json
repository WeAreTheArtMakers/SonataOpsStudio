{
  "id": null,
  "uid": "rag-quality",
  "title": "SonataOps - RAG Quality",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-7d", "to": "now" },
  "panels": [
    {
      "type": "stat",
      "title": "RAG Eval Pass Rate",
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "rag_eval_pass_rate", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "unit": "percentunit"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "RAG Queries",
      "gridPos": { "h": 8, "w": 16, "x": 8, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "sum(rate(rag_queries_total[5m]))", "refId": "A" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Copilot API Latency",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{route=~'.*copilot.*'}[5m])) by (le))", "refId": "A" }
      ]
    }
  ],
  "templating": { "list": [] },
  "annotations": { "list": [] }
}
